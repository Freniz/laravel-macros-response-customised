{
    "name": "freniz/laravel-macros-response-customised",
    "description": "Customised Response Macro's for Laravel",
    "keywords": ["freniz", "customised","laravel", "response", "macros", "php"],
    "license": "MIT", 
    "homepage": "http://www.freniz.com",
    "authors": [
        {
            "name": "Abdul Nizam",
            "email": "abdulnizam@freniz.com",
            "homepage": "http://www.abdulnizam.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6",
        "laravel/framework": ">=5.4"
    },
    "autoload": {
        "psr-4": {
            "Freniz\\ResponseMacros\\": "src"
        }
    },
     "config": {
        "sort-packages": true
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('config/laravelmacros.php') || copy('/../../../../config/laravelmacros.php', 'config/laravelmacros.php');\""
        ],
        "post-install-cmd": [
            "php artisan vendor:publish --provider=\"freniz\\laravel-macros-response-customised\\ResponseMacrosServiceProvider\" --tag=laravelmacros"
        ],
        "post-update-cmd": [
            "php artisan vendor:publish --provider=\"freniz\\laravel-macros-response-customised\\ResponseMacrosServiceProvider\" --tag=laravelmacros"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Freniz\\ResponseMacros\\ResponseMacrosServiceProvider"
            ]
        }
    }
}
